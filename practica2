#graficar pie la variables calificaion
##incorporar los diversos argumentos utilizados en clase
fac<-c ("economia","ingenieria","arquitectura","derecho")
calif <-c (6,4,10,8)
lic<-c("Actuaria","ing. civil","arquitectura","derecho")
class(fac)
class(lic)
class(calif)
rm(fac)
rm(lic)
pie (calif, lic, col = c ("blue", "red", "pink", "yellow"), radius = 1, clockwise = TRUE, lty = 18)
